@model List<Diyetiva.WebUI.Dtos.ProductDtos.ResultProductDto>
<div class="tab-content" data-aos="fade-up" data-aos-delay="200">


    <div class="tab-pane fade active show" id="menu-starters">

        <div class="tab-header text-center">
            <p>Beslenme Planı</p>
            <h3>Tüm Öğünler</h3>
        </div>

        <div class="row gy-5">
            @foreach (var item in Model)
            {

                <div class="col-lg-4 menu-item">
                    <a href="@item.ImageUrl" class="glightbox"><img src="@item.ImageUrl" class="menu-img img-fluid" alt=""></a>
                    <h4>@item.ProductName</h4>
                    <p class="ingredients">
                        @item.Description
                    </p>
                  
                </div><!-- Menu Item -->


            }
        </div>
        </div>

        <!-- End Starter Menu Content -->
    
   

   

</div>